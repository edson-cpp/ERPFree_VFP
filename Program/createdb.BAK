*! @field CPF
*! @field idusugru = Identificação do registro do grupo de usuários
Create Table 'Usuários' Name 'usuario'(;
	id			Integer Autoinc,;
	login		Character(40),;
	senha		Character(32),;
	unome		Character(50),;
	cpf			Character(14),;
	idusugru	Integer;
	)
