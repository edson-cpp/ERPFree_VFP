	comdesc		Logical,;
	commulta	Logical,;
	comjuros	Logical,;
	datadesc	Date,;
	vlrdesc		Float(10,2),;
	perdesc		Float(6,2),;
	vlratraso	Float(10,2),;
	peratraso	Float(6,2),;
	vlrjuros	Float(10,2),;
	perjuros	Float(6,2),;
	datapag		Date,;
	idvenda		Integer,;
	idcontas	Integer;
