	id			Integer Autoinc,;
	idcliente	Integer,;
	datalan		Date,;
	situ		Character(1),;
	idcontorc	Integer,;
	idcentcus	Integer,;
	numdoc		Character(15),;
	idtipdoc	Integer,;
	emissao		Date,;
	vencimento	Date,;
